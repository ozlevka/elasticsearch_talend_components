<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER 
  	AUTHOR="Ozeryansky Lev" 
  	COMPATIBILITY="ALL" 
  	DATA_AUTO_PROPAGATE="false"
  	RELEASE_DATE="20140515A" 
  	SERIAL="" 
  	STARTABLE="false" 
  	STATUS="ALPHA"
  	PLATEFORM="ALL"
  	EXTENSION="org.talend.designer.fileoutputxml" 
  	SCHEMA_AUTO_PROPAGATE="false"
  	PARTITIONING="NONE" 
  	VERSION="0.001"> 
    <SIGNATURE/> 
  </HEADER>
  <FAMILIES> 
    <FAMILY>elasticsearch</FAMILY> 
  </FAMILIES>  
  <DOCUMENTATION> 
    <URL/> 
  </DOCUMENTATION>  
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
	<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
	<CONNECTOR CTYPE="COMPONENT_OK"/>
	<CONNECTOR CTYPE="COMPONENT_ERROR"/>
  </CONNECTORS>  
  <PARAMETERS> 
    <PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" REQUIRED="true" NUM_ROW="10">
			<DEFAULT />
	</PARAMETER>
	<PARAMETER NAME="ROOT" FIELD="TABLE" REQUIRED="true"
			NUM_ROW="11" NB_LINES="3" SHOW="false">
			<ITEMS>
				<ITEM NAME="PATH" FIELD="TEXT" />
				<ITEM NAME="COLUMN" FIELD="TEXT" />
				<ITEM NAME="VALUE" FIELD="TEXT" />
				<ITEM NAME="ATTRIBUTE" FIELD="TEXT" />
				<ITEM NAME="ORDER" FIELD="TEXT" />
			</ITEMS>
		</PARAMETER>
	<PARAMETER NAME="MAP" FIELD="EXTERNAL" NUM_ROW="20">
			<DEFAULT />
	</PARAMETER>
	<PARAMETER NAME="HOST" FIELD="TEXT"  REQUIRED="true" NUM_ROW="30">
		<DEFAULT>localhost</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="PORT" FIELD="TEXT" REQUIRED="true" NUM_ROW="40">
		<DEFAULT>9200</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="INDEX" FIELD="TEXT" REQUIRED="true" NUM_ROW="50">
		<DEFAULT></DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="DOC_TYPE" FIELD="TEXT" REQUIRED="true" NUM_ROW="60">
		<DEFAULT></DEFAULT>
	</PARAMETER>
  </PARAMETERS>  
  <ADVANCED_PARAMETERS> 
    <PARAMETER FIELD="TEXT" NAME="PROPERTYADVANCED" NUM_ROW="1"/> 
  </ADVANCED_PARAMETERS>  
  <CODEGENERATION>
  	<TEMPLATES INPUT="In" OUTPUT="Out">
		<TEMPLATE NAME="In" COMPONENT="tWriteXMLFieldOut">
			<LINK_TO NAME="OUT" CTYPE="COMPONET_OK"/>
		</TEMPLATE>
		<TEMPLATE_PARAM SOURCE="self.ROOT" TARGET="In.ROOT" />
	</TEMPLATES> 
    <IMPORTS> 
      <IMPORT MODULE="elasticwrapper.jar" NAME="elasticwrapper" REQUIRED="true"/> 
    </IMPORTS> 
  </CODEGENERATION>  
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
  </RETURNS> 
</COMPONENT>
